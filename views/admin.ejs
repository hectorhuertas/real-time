<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Admin View</title>
  </head>
  <body>
    <h1>Poll Results</h1>

    <h2><%= poll.title %></h2>

    <% for(option in poll.options){ %>
      <p><%= option %>: <span><%= poll.options[option] %></span></p>
    <% } %>

    <script src="/jquery-2.2.3.js"> </script>
    <script src="/socket.io/socket.io.js"> </script>
    <script src="/admin.js"> </script>
  </body>
</html>
